.tabNav{z-index: 4; position: fixed; top:80rpx;left:0; width:100%; height:100rpx; line-height: 100rpx; background: #fff; display: flex; padding:0 30rpx; border-bottom:1px solid #f5f5f5; box-sizing: border-box; justify-content: space-around}
.tabNav> view{text-align: center;width:33.3%; }
.tabNav> view:last-child{ margin-right: 0;}
.tabNav> view text{padding:0 15rpx; height:75rpx; display:inline-block;font-size:36rpx;padding-bottom:20rpx;
}
.tabNav .cur text{ border-bottom:5rpx solid #0085e2; color:#0085e2;}
