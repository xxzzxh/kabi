/* 不含表单项目 */
.noneimage{
  display: block;
  text-align: center;
  margin-top:25%;
  height:500rpx;
  width: 100%
}
.noneimage image{
height:100rpx;
  width:100rpx;
}
.noneimage text{
  display: block
}

/* 含表单项目 */
.title{
  margin-left: 10rpx;
}
.formbox{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content:center;

}
.formboxitem{
  display: block;
  width:100%;
  height:60px;
  background:white;
  color:#1473d2;
  margin-top:4%;
  width:94%;
  box-shadow:#00000010 0px 3px 30px;
  border-radius: 6px;
}


.formname{
  text-align: 30px;
  text-align: center;
  font-size: 18px;
}
.formtime{
  text-align: 15px;
  font-size: 10px;
  margin-right: 6px;
  text-align: center;
  color: #B0B0B0;
}
.formstatus{
  text-align: 15px;
  font-size: 10px;
  text-align: right;
  color: #B0B0B0;
  margin-right: 4px;
}
.formstatusno{
  text-align: 15px;
  font-size: 10px;
  text-align: right;
  color: #FF0000;
  margin-right: 4px;
}